{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.10",
        "installCommand": "python -m pip install --upgrade pip setuptools wheel && pip install -r requirements.txt",
        "maxLambdaSize": "250mb"
      }
    }
  ],
  "routes": [
    {"src": "/static/(.*)", "dest": "/static/$1"},
    {"src": "/(.*)", "dest": "app.py"}
  ]
}