{
"version": 2,
"builds": [
  {
    "src": "app/wsgi.py",
    "use": "@vercel/python",
    "config": {
      "maxLambdaSize": "15mb",
      "runtime": "python 3.12.6"
    }
  },
  {
    "src": "build_files.sh",
    "use": "@vercel/static-build",
    "config": {
      "distDir": "staticfiles"
    }
  }
],
"routes": [
  {
    "src": "/(.*)",
    "dest": "app/wsgi.py"
  }
],
"outputDirectory": "staticfiles"
}