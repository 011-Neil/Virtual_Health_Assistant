{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  
    "functions": [{
      "app/**": {
        "excludeFiles": "{__pycache__,nltk_data,*.cache,node_modules,public,app}/**"
      }
    }]
  
}
